{
  "version": 2,
  "name": "zeit-now",
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ],
  "env": {
    "DB_HOST":"@db_host",
    "DB_PORT":"@db_port",
    "DB_NAME":"@db_name",
    "DB_USER":"@db_user",
    "DB_PASSWORD":"@db_password",
    "SECRET_KEY_JWT":"@secret-key-jwt",
    "TOKEN_EXPIRATION_TIME":"@token-expiration-time"
  }
}
